<div class="span-21" id="header">
  <table>
    <tr><td>
      <h2>New Entries</h2>
      <table>
      <% @entries.each do |e| %>
        <tr><td><%= e.name %></td></tr>
      <% end %>
      </table>
    </td><td>
      <h2>New Scores</h2>
      <table>
      <% @scores.each do |e| %>
        <tr><td><%= e.score %></td></tr>
      <% end %>
      </table>
    </td></tr>
  </table>
</span>

<div class="span-21">
  <h2>Latest Pushes</h2>
  <table>
  <% @pushes.each do |e| %>
    <tr><td><%= e.attributes %></td></tr>
  <% end %>
  </table>
</div>